{
  "name": "aoe-stats-api",
  "version": "1.0.0",
  "description": "Age of Empires statistics API with modern parquet reading",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seed.js",
    "incremental-seed": "node scripts/incremental-seed.js",
    "test": "node scripts/modern-test.js",
    "test-connection": "node scripts/test-connection.js",
    "redis:start": "docker-compose up -d redis",
    "redis:start-with-web": "docker-compose --profile tools up -d",
    "redis:stop": "docker-compose down",
    "redis:restart": "docker-compose restart redis",
    "redis:logs": "docker-compose logs -f redis",
    "redis:status": "docker-compose ps",
    "redis:clean": "docker-compose down -v",
    "dev:full": "npm run redis:start-with-web && npm run dev"
  },
  "keywords": ["age-of-empires", "statistics", "api", "parquet", "mongodb"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "axios": "^1.5.0",
    "parquet-wasm": "^0.6.0",
    "apache-arrow": "^13.0.0",
    "redis": "^4.6.0",
    "dotenv": "^16.3.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}